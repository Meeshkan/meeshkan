					}
				}
			}
		},
		"og-cards-e2e": {
			"root": "apps/og-cards-e2e",
			"sourceRoot": "apps/og-cards-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/og-cards-e2e/cypress.json",
						"tsConfig": "apps/og-cards-e2e/tsconfig.e2e.json",
						"devServerTarget": "og-cards:serve"
					},
					"configurations": {
						"production": {
