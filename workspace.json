					"configurations": {
						"production": {
							"devServerTarget": "website:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/website-e2e/**/*.{js,ts}"]
					}
				}
			}
		},
		"chakra-theme": {
