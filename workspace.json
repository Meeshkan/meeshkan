				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/og-cards-e2e/**/*.{js,ts}"]
					}
				}
			}
		},
		"meeshkan-types": {
			"root": "libs/meeshkan-types",
			"sourceRoot": "libs/meeshkan-types/src",
			"projectType": "library",
			"schematics": {},
			"architect": {
				"lint": {
