				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"options": {
						"jestConfig": "libs/downloadable-script/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		}
	},
	"cli": {
		"defaultCollection": "@nrwl/next"
	},
	"schematics": {
		"@nrwl/react": {
			"application": {
				"babel": true
			}
		},
		"@nrwl/next": {
			"application": {
				"style": "css",
				"linter": "eslint"
			}
		},
		"@nrwl/gatsby": {
			"application": {}
		}
	},
	"defaultProject": "webapp"
}
